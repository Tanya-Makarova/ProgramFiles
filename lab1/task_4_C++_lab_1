#Дан массив цифр от 1 до 10 в произвольном порядке. Необходимо разбить его на два списка: в первом цифры будут от 1 до 5, а во втором 6 до 10.
#include <iostream>
#include <stack>
#include <vector>
using namespace std;

int main() {
    // Исходный массив цифр от 1 до 10
    vector<int> numbers = { 2, 1, 3, 9, 5, 6, 8, 7, 4, 10 };

    // Создаем два стека для разделения чисел
    stack<int> stack1; // для чисел 1-5
    stack<int> stack2; // для чисел 6-10

    // Проходим по всем числам в массиве
    for (int num : numbers) {
        if (num <= 5) {
            stack1.push(num); // Добавляем в первый стек если число <= 5
        }
        else {
            stack2.push(num); // Добавляем во второй стек если число > 5
        }
    }

    // Создаем векторы для хранения результатов
    vector<int> list1;
    vector<int> list2;

    // Извлекаем элементы из первого стека и сохраняем в list1
    while (!stack1.empty()) {
        list1.push_back(stack1.top()); // Берем верхний элемент
        stack1.pop(); // Удаляем его из стека
    }

    // Извлекаем элементы из второго стека и сохраняем в list2
    while (!stack2.empty()) {
        list2.push_back(stack2.top()); // Берем верхний элемент
        stack2.pop(); // Удаляем его из стека
    }

    // Выводим результаты
    cout << "First list (1-5): ";
    for (int num : list1) {
        cout << num << " ";
    }
    cout << endl;

    cout << "Second list (6-10): ";
    for (int num : list2) {
        cout << num << " ";
    }
    cout << endl;

    return 0;
}
